$TTL 3600
$ORIGIN dnsnew.lab.
@               IN      SOA     ns01.dns.lab. root.dns.lab. (
                            2019081601 ; serial
                            3600       ; refresh (1 hour)
                            600        ; retry (10 minutes)
                            86400      ; expire (1 day)
                            600        ; minimum (10 minutes)
                        )

                IN      NS      ns01.dnsnew.lab.
                IN      NS      ns02.dnsnew.lab.

; DNS Servers
ns01            IN      A       192.168.50.10
ns02            IN      A       192.168.50.11
; Clients
www             IN      A       192.168.50.15
                IN      A       192.168.50.25